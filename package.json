{
  "version": "0.0.13",
  "main": "dist/index.bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanonelson/redux-endpoints"
  },
  "scripts": {
    "build": "webpack --colors --progress",
    "build:prd": "NODE_ENV=production npm run build",
    "build:test": "npm run build -- --config=webpack.config.test.js",
    "dev": "webpack-dev-server",
    "start:tmux": "tmux split-window \"npm run watch:test\" && tmux split-window \"npm run watch\" && tmux select-layout main-vertical && npm run test:server",
    "test": "mocha ./test/dist/",
    "pretest": "npm run build:prd && npm run build:test",
    "test:server": "mocha ./test/dist/ -w",
    "test:browser": "open ./test/dist/index.html",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run build:test -- --watch"
  },
  "author": "Dylan Nelson",
  "license": "ISC",
  "name": "redux-endpoints",
  "description": "Redux modules for API endpoints",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "file-loader": "^0.11.2",
    "jest": "^20.0.4",
    "mocha": "^3.4.2",
    "prop-types": "^15.5.10",
    "redux": "^3.7.2",
    "style-loader": "^0.18.2",
    "url-parse": "^1.1.9",
    "webpack": "^3.4.0",
    "webpack-dev-server": "^2.6.1"
  }
}
